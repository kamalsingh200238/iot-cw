{
  "name": "iot",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "tailwind": "npx tailwindcss -i ./input.css -o ./main.css",
    "tailwind-watch": "npx tailwindcss -i ./input.css -o ./main.css --watch",
    "format": "npx prettier -w .",
    "php": "php -S localhost:8000",
    "dev": "npx concurrently --kill-others \"pnpm tailwind-watch\" \"pnpm php\""
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.7",
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "postcss": "^8.4.38",
    "rustywind": "^0.22.0",
    "tailwindcss": "^3.4.3"
  }
}
